<% if orders.present? %>
  <% if params[:t] == 'w'%>
  	$('#tab1 table tbody').html("<%= escape_javascript(render :partial => 'table', :locals => { :orders => @orders }) %>");
  <% elsif params[:t] == 'n'%>
  	$('#tab2 table tbody').html("<%= escape_javascript(render :partial => 'table', :locals => { :orders => @orders }) %>");
  <% elsif params[:t] == 'p'%>
  	$('#tab3 table tbody').html("<%= escape_javascript(render :partial => 'table', :locals => { :orders => @orders }) %>");
  <% else %>
  	$('#tab4 table tbody').html("<%= escape_javascript(render :partial => 'table', :locals => { :orders => @orders }) %>");
  <% end %>
  <% if orders['data'].length <= 0 %>
  	$('td.no-data').text('未查到相关记录');
  <% end %>  
<% end %>