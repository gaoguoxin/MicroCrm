<% if users.present? %>
  $('#tab1 table tbody').html("<%= escape_javascript(render :partial => 'table', :locals => { :users => @users }) %>");
  $('#tab1 .page-container').html("<%= escape_javascript(admin_paginator_ajax('user',users,{:name => params[:name],:email => params[:email],:role => params[:role],
      :position => params[:position],:company => params[:company],:city => params[:city],
      :interest => params[:interest],:creater => params[:creater],:status => params[:status],:start => params[:start],:end => params[:end]})) %>");
  <% if users['data'].length <= 0 %>
  	$('td.no-data').text('未查到符合条件的相关学员,请确认查询条件是否正确!');
  <% end %>  
<% end %>
