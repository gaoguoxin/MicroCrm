<% if orders.present? %>
  $('table tbody').html("<%= escape_javascript(render :partial => 'table', :locals => { :orders => @orders }) %>");
  $('.page-container').html("<%= escape_javascript(admin_paginator_ajax('order',orders,{:code => params[:code],:name => params[:name],:start => params[:start],:state => params[:state],:status => params[:status],:cancel => params[:cancel],:content => params[:content],:city => params[:city],:company => params[:company],:t => params[:t]})) %>");
  <% if orders['data'].length <= 0 %>
  	$('td.no-data').text('未查到符合条件的报名记录,请确认查询条件是否正确!');
  <% end %>  
<% end %>
