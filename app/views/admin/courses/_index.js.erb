
<% if courses.present? %>
  $('table tbody').html("<%= escape_javascript(render :partial => 'table', :locals => { :courses => @courses }) %>");
  $('.page-container').html("<%= escape_javascript(admin_paginator_ajax('course',courses,{:code => params[:code],:name => params[:name],:start => params[:start],:end => params[:end],:content => params[:content],:city => params[:city],:status => params[:status]})) %>");
  <% if courses['data'].length <= 0 %>
  	$('td.no-data').text('未查到符合条件的课程,请确认查询条件是否正确!');
  <% end %>  
<% end %>